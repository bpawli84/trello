<h1>Editing note</h1>
 
<%= form_for :note, url: note_path(@note), method: :patch do |f| %>
 
 
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
<button> 
<%= link_to 'Back', notes_path %>
</button>